[ req ]
default_bits = 4096
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no
default_md = sha1

[ req_dn ]
C=FR
ST=FR
L=Paris
O=Courier Mail Server
OU=Automatically-generated IMAP SSL key
CN=imaps.zamaroczy.fr
emailAddress=postmaster@zamaroczy.fr


[ cert_type ]
nsCertType = server

